{
  "name": "@openai/i18n",
  "version": "1.0.0",
  "description": "Internationalization package for OpenAI monorepo",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "typecheck": "tsc --noEmit",
    "extract": "i18next -c i18next-parser.config.cjs",
    "extract:watch": "i18next -c i18next-parser.config.cjs --watch"
  },
  "dependencies": {
    "i18next": "^23.7.6",
    "react-i18next": "^14.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "i18next-parser": "^9.0.0",
    "typescript": "^5.3.2"
  }
}
